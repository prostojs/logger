{
  "name": "@prostojs/logger",
  "version": "0.3.7",
  "description": "Plain simple logger with banner and colors",
  "main": "index.js",
  "module": "dist/logger.esm-bundler.mjs",
  "types": "dist/logger.d.ts",
  "files": [
    "index.js",
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/logger.cjs.prod.js",
      "import": "./dist/logger.esm-bundler.mjs",
      "types": "./dist/logger.d.ts"
    }
  },
  "scripts": {
    "build": "node ./scripts/build",
    "release": "node ./scripts/release",
    "test": "jest --runInBand",
    "test:cov": "jest --runInBand --coverage",
    "lint": "eslint --ext .ts src/**.ts --fix",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prostojs/logger.git"
  },
  "keywords": [
    "logger",
    "prostojs"
  ],
  "buildOptions": {
    "name": "ProstoLogger",
    "formats": [
      "esm-bundler",
      "cjs"
    ]
  },
  "gitHooks": {
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "author": "Artem Maltsev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prostojs/logger/issues"
  },
  "homepage": "https://github.com/prostojs/logger#readme",
  "devDependencies": {
    "@prostojs/dye": "^0.3.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "brotli": "^1.3.3",
    "conventional-changelog": "^3.1.24",
    "conventional-changelog-cli": "^2.1.1",
    "enquirer": "^2.3.6",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "execa": "^5.1.1",
    "fs-extra": "^11.2.0",
    "jest": "^29.7.0",
    "minimist": "^1.2.8",
    "rollup": "^4.9.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "semver": "^7.3.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "yorkie": "^2.0.0",
    "zlib": "^1.0.5"
  }
}
